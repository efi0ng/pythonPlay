<?xml version="1.0" encoding="UTF-8"?>
<tmx
  version="1.4"
  xmlns:tal="http://xml.zope.org/namespaces/tal">
  <header
    creationtool="Resx2tmx"
    creationtoolversion="${document.tool_version}"
    segtype="sentence"
    o-tmf="MadCap Lingo Translation Memory"
    adminlang="en-US"
    srclang="*all*"
    datatype="x-sample"
    creationdate="${document.datetime}" />
  <body>
    <tu tal:repeat="item items"
      creationid="resx2tmx"
      creationdate="${document.datetime}">
      <tuv
        xml:lang="${item.source_lang}"
        changedate="${document.datetime}"
        creationdate="${document.datetime}"
        creationid="resx2tmx"
        changeid="resx2tmx">
          <prop type="x-project">Resx2Tmx</prop>
          <prop type="x-document">${document.target_doc}</prop>
          <seg>${item.source_text}</seg>
      </tuv>
      <tuv
        xml:lang="${item.target_lang}"
        changedate="${document.datetime}"
        creationdate="${document.datetime}"
        creationid="resx2tmx"
        changeid="resx2tmx">
            <prop type="x-project">Resx2Tmx</prop>
            <prop type="x-document">${document.target_doc}</prop>
            <seg>${item.target_text}</seg>
        </tuv>
    </tu>
  </body>
</tmx>